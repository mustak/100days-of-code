

<div class="container">
<h1>All users</h1>
<ul id='users'>
    <% users.forEach(user =>{ %>
        <li>
            <div class='avatar'><img src="/<%= user.avatar %>" alt="image of <%= user.name %>"></div>
            <div><%= user.name %></div>
            <form method='post' action="<%= `${user._id}/delete` %>">
            <button type='submit'>Delete</button>
            </form>
            </li>
    <%}) %>
   
</ul>
</div>